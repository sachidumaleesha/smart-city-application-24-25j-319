{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "server/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "client/$1"
      }
    ]
  }
  